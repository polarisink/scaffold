<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns="http://maven.apache.org/POM/4.0.0"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
  <artifactId>scaffold</artifactId>
  <description>自定义脚手架</description>
  <groupId>github.polarisink</groupId>
  <modelVersion>4.0.0</modelVersion>
  <modules>
    <module>scaffold-common</module>
    <module>scaffold-api</module>
    <module>scaffold-dao</module>
    <module>scaffold-cache</module>
    <module>scaffold-third</module>
    <module>scaffold-mqtt</module>
  </modules>
  <name>scaffold</name>

  <packaging>pom</packaging>
  <properties>
    <maven.compiler.source>17</maven.compiler.source>
    <maven.compiler.target>17</maven.compiler.target>
    <java.version>17</java.version>
    <spring-boot.version>2.7.10</spring-boot.version>
    <spring-cloud.version>2021.0.6</spring-cloud.version>
    <skipTests>true</skipTests>
    <!--docker地址和各个插件的信息-->
    <bcprov-jdk15on.version>1.69</bcprov-jdk15on.version>
    <bcrypt.version>0.9.0</bcrypt.version>
    <caffeine.version>3.1.1</caffeine.version>
    <docker-maven.version>0.4.14</docker-maven.version>
    <druid.version>1.2.11</druid.version>
    <easy-es.version>0.9.60</easy-es.version>
    <freemarker.version>2.3.31</freemarker.version>
    <gelf.version>1.3.1</gelf.version>
    <!--常用依赖-->
    <guava.version>31.1-jre</guava.version>
    <hutool-all.version>5.8.10</hutool-all.version>
    <jakarta-validation.version>2.0.2</jakarta-validation.version>
    <lombok-mapstruct-binding.version>0.2.0</lombok-mapstruct-binding.version>
    <lombok.version>1.18.24</lombok.version>
    <mapstruct.spring.version>0.1.2</mapstruct.spring.version>
    <mariadb.version>3.1.0</mariadb.version>
    <mqtt.version>1.2.5</mqtt.version>
    <maven.version>3.8.1</maven.version>
    <minio.version>8.3.7</minio.version>
    <mybatis-plus.version>3.5.3.1</mybatis-plus.version>
    <mybatisplus-gen.version>3.5.2</mybatisplus-gen.version>
    <mysql.version>8.0.30</mysql.version>
    <okhttp.version>4.10.0</okhttp.version>
    <ons.version>2.0.3.Final</ons.version>
    <org.mapstruct.version>1.5.3.Final</org.mapstruct.version>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    <project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
    <rocketmq.version>2.2.2</rocketmq.version>
    <junit.version>4.13.2</junit.version>
    <netty.version>4.1.86.Final</netty.version>
    <!--打包对test只编译不运行-->
  </properties>
  <version>0.0.1-SNAPSHOT</version>

  <dependencyManagement>
    <dependencies>
      <dependency>
        <artifactId>spring-cloud-dependencies</artifactId>
        <groupId>org.springframework.cloud</groupId>
        <scope>import</scope>
        <type>pom</type>
        <version>${spring-cloud.version}</version>
      </dependency>
      <dependency>
        <artifactId>spring-boot-dependencies</artifactId>
        <groupId>org.springframework.boot</groupId>
        <scope>import</scope>
        <type>pom</type>
        <version>${spring-boot.version}</version>
      </dependency>
      <!--<dependency>
        <artifactId>druid-spring-boot-starter</artifactId>
        <groupId>com.alibaba</groupId>
        <version>${druid.version}</version>
      </dependency>-->
      <dependency>
        <artifactId>mybatis-plus-boot-starter</artifactId>
        <groupId>com.baomidou</groupId>
        <version>${mybatis-plus.version}</version>
      </dependency>
      <dependency>
        <artifactId>mybatis-plus-generator</artifactId>
        <groupId>com.baomidou</groupId>
        <version>${mybatisplus-gen.version}</version>
      </dependency>
      <dependency>
        <artifactId>mysql-connector-java</artifactId>
        <groupId>mysql</groupId>
        <scope>runtime</scope>
        <version>${mysql.version}</version>
      </dependency>
      <dependency>
        <artifactId>lombok</artifactId>
        <groupId>org.projectlombok</groupId>
        <version>${lombok.version}</version>
      </dependency>
      <dependency>
        <artifactId>minio</artifactId>
        <exclusions>
          <exclusion>
            <artifactId>okhttp</artifactId>
            <groupId>com.squareup.okhttp3</groupId>
          </exclusion>
        </exclusions>
        <groupId>io.minio</groupId>
        <version>${minio.version}</version>
      </dependency>
      <dependency>
        <artifactId>okhttp</artifactId>
        <groupId>com.squareup.okhttp3</groupId>
        <version>${okhttp.version}</version>
      </dependency>
      <dependency>
        <artifactId>junit</artifactId>
        <groupId>junit</groupId>
        <version>${junit.version}</version>
      </dependency>
      <dependency>
        <artifactId>hutool-all</artifactId>
        <groupId>cn.hutool</groupId>
        <version>${hutool-all.version}</version>
      </dependency>
      <!-- mapStruct 核心注解 -->
      <dependency>
        <artifactId>mapstruct</artifactId>
        <groupId>org.mapstruct</groupId>
        <version>${org.mapstruct.version}</version>
      </dependency>
      <!--<dependency>
        <artifactId>easy-es-boot-starter</artifactId>
        <groupId>cn.easy-es</groupId>
        <version>${easy-es.version}</version>
      </dependency>-->
      <dependency>
        <artifactId>rocketmq-spring-boot-starter</artifactId>
        <groupId>org.apache.rocketmq</groupId>
        <version>${rocketmq.version}</version>
      </dependency>
      <!-- mapStruct 根据接口生成实现类 -->
      <dependency>
        <artifactId>mapstruct-processor</artifactId>
        <groupId>org.mapstruct</groupId>
        <scope>provided</scope>
        <version>${org.mapstruct.version}</version>
      </dependency>
      <!-- mapStruct 支持 lombok -->
      <dependency>
        <artifactId>lombok-mapstruct-binding</artifactId>
        <groupId>org.projectlombok</groupId>
        <scope>provided</scope>
        <version>${lombok-mapstruct-binding.version}</version>
      </dependency>
      <!--spring mapstruct插件-->
      <dependency>
        <artifactId>mapstruct-spring-annotations</artifactId>
        <groupId>org.mapstruct.extensions.spring</groupId>
        <version>${mapstruct.spring.version}</version>
      </dependency>
      <dependency>
        <artifactId>jakarta.validation-api</artifactId>
        <groupId>jakarta.validation</groupId>
        <version>${jakarta-validation.version}</version>
      </dependency>
      <dependency>
        <artifactId>bcrypt</artifactId>
        <groupId>at.favre.lib</groupId>
        <version>${bcrypt.version}</version>
      </dependency>
      <dependency>
        <artifactId>freemarker</artifactId>
        <groupId>org.freemarker</groupId>
        <version>${freemarker.version}</version>
      </dependency>
      <dependency>
        <groupId>jakarta.servlet</groupId>
        <artifactId>jakarta.servlet-api</artifactId>
        <version>6.0.0</version>
      </dependency>
      <dependency>
        <groupId>org.eclipse.paho</groupId>
        <artifactId>org.eclipse.paho.mqttv5.client</artifactId>
        <version>${mqtt.version}</version>
      </dependency>
    </dependencies>
  </dependencyManagement>

  <build>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <configuration>
          <annotationProcessorPaths>
            <path>
              <artifactId>lombok</artifactId>
              <groupId>org.projectlombok</groupId>
              <version>${lombok.version}</version>
            </path>
            <path>
              <artifactId>mapstruct-processor</artifactId>
              <groupId>org.mapstruct</groupId>
              <version>${org.mapstruct.version}</version>
            </path>
            <path>
              <artifactId>mapstruct-spring-extensions</artifactId>
              <groupId>org.mapstruct.extensions.spring</groupId>
              <version>
                ${mapstruct.spring.version}
              </version>
            </path>
          </annotationProcessorPaths>
          <encoding>utf8</encoding>
          <source>${java.version}</source>
          <target>${java.version}</target>
        </configuration>
        <groupId>org.apache.maven.plugins</groupId>
        <version>${maven.version}</version>
      </plugin>
    </plugins>
  </build>
</project>